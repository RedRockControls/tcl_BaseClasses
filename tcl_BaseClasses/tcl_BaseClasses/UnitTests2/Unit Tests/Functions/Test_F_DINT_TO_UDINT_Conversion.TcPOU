  <POU Name="Test_F_DINT_TO_UDINT_Conversion" Id="{7618d050-2f09-4dc1-a5bb-d62f65f5c08f}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK Test_F_DINT_TO_UDINT_Conversion EXTENDS T_UnitTestBase
