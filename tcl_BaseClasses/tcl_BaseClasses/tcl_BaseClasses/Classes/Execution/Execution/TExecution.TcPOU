    <Method Name="SetAborted" Id="{3457d55d-8917-427f-b4b1-3d8dae8fe5f2}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD SetAborted // used when implementing class signals Aborted
VAR
    EventData : T_MaxString;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _State = TExecutionState.Starting OR _State = TExecutionState.Busy THEN
    _Aborted := TRUE;
    _State := TExecutionState.Ready;
    
    IF _Event_OnAbort <> 0 THEN
        _Event_OnAbort.Args.Add_STRING(F_FormatString1('Execution aborted in %s',F_STRING(Name)));
        _Event_OnAbort.Raise();
    END_IF  

    IF Message_ExecutionAborted.nEventId = 0 THEN
        Message_ExecutionAborted.CreateEx(
            stEventEntry := TC_EVENTS.tcl_BaseClassEvents.ExecutionAborted,
            ipSourceInfo := 0);
    END_IF
    Message_ExecutionAborted.Send(0);
END_IF

IF _State = TExecutionState.Aborting THEN
    _Aborted := TRUE;
    _State := TExecutionState.Ready;
END_IF

]]></ST>
      </Implementation>
    </Method>
    <Method Name="SetBusy" Id="{a5a7c695-c8c2-0fa3-058b-7078b33d95e5}" FolderPath="Methods\">
