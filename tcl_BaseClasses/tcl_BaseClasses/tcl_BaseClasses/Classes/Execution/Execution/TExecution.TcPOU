END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _State = TExecutionState.Starting OR _State = TExecutionState.Busy THEN
    _Done := TRUE;
    _State := TExecutionState.Ready;
END_IF
]]></ST>
      </Implementation>
    </Method>
    <Method Name="SetError" Id="{6247352f-ee68-037a-2fe3-925d2cb9b25d}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD SetError
VAR_INPUT
    ErrorId : UDINT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _State = TExecutionState.Starting OR _State = TExecutionState.Busy THEN
    _Error   := TRUE;
    _ErrorId := ErrorId;
    _State   := TExecutionState.Ready;
    
    IF _Event_OnError <> 0 THEN
        _Event_OnError.Args.Add_STRING(F_FormatString2('Error 0x%0.4X in %s',F_UDINT(_ErrorId),F_STRING(Name)));
        _Event_OnError.Raise();
    END_IF
    
    IF Message_ExecutionError.nEventId = 0 THEN
        Message_ExecutionError.CreateEx(
            stEventEntry := TC_EVENTS.tcl_BaseClassEvents.ExecutionError,
            ipSourceInfo := 0);
	END_IF
    Message_ExecutionError.ipArguments.Clear().AddUDInt(_ErrorId);
    Message_ExecutionError.Send(0);
END_IF
]]></ST>
      </Implementation>
    </Method>
    <Method Name="Start" Id="{def3bafe-9648-0b74-364e-7150751f5a3a}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD Start : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _State = TExecutionState.Ready  THEN
    _Done    := FALSE;
    _Aborted := FALSE;
    _Error   := FALSE;
    _ErrorId := 0;
    
    _State   := TExecutionState.Starting;
    Start    := TRUE; // Accepted
ELSE
    IF _Event_OnError <> 0 THEN
        _Event_OnError.Args.Add_STRING(F_FormatString1(
                                        Format := 'Start requested while not ready in %s',
                                        Arg1   := F_STRING(Name)));
        _Event_OnError.Raise();
    END_IF
    
    IF Message_ExecutionNotReady.nEventId = 0 THEN
        Message_ExecutionNotReady.CreateEx(
            stEventEntry := TC_EVENTS.tcl_BaseClassEvents.ExecutionNotReady,
            ipSourceInfo := 0);
    END_IF
    Message_ExecutionNotReady.Send(0);
    
END_IF

]]></ST>
      </Implementation>
    </Method>
