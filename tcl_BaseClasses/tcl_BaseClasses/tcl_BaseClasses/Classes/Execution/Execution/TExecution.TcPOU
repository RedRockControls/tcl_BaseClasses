    <Method Name="Clear" Id="{e820ac7a-e3e5-421e-9394-79fc023e1ef0}" FolderPath="Methods\">
      <Declaration><![CDATA[// Clears the results flags provided State = Ready
METHOD Clear 

]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _State = TExecutionState.Ready  THEN
    _Done    := FALSE;
    _Aborted := FALSE;
    _Error   := FALSE;
    _ErrorId := 0;
    
ELSE
    IF _Event_OnError <> 0 THEN
        _Event_OnError.Args.Add_STRING(F_FormatString1(
                                        Format := 'Clear requested while not ready in %s',
                                        Arg1   := F_STRING(Name)));
        _Event_OnError.Raise();
    END_IF
END_IF
]]></ST>
      </Implementation>
    </Method>
