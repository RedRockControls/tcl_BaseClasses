    <Method Name="Abort" Id="{b3fb44cc-0bf3-039d-3269-899cc8ea6ebc}" FolderPath="Methods\">
      <Declaration><![CDATA[// Requests that the current operation be aborted - set the State to Aborting
METHOD Abort 
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _State <> TExecutionState.Ready THEN
    _State := TExecutionState.Aborting; 
    // Aborting must be handled by parent class - cleanup and set Ready state
END_IF
]]></ST>
      </Implementation>
    </Method>
